          "title": "Wikimedia Pageviews",
          "base": "https://wikimedia.org/api/rest_v1/metrics/pageviews/per-article/{project}/{article}/{access}/{agent}/{granularity}/{start}/{end}",
          "example": "https://wikimedia.org/api/rest_v1/metrics/pageviews/per-article/en.wikipedia.org/Albert_Einstein/all-access/user/daily/20240101/20251231",
          "cors": "yes",
          "format": "json"
        },
        {
          "key": "wikipedia_summary",
          "title": "Wikipedia Article Summary",
          "base": "https://en.wikipedia.org/api/rest_v1/page/summary/{title}",
          "example": "https://en.wikipedia.org/api/rest_v1/page/summary/Albert_Einstein",
          "cors": "yes",
          "format": "json"
        },
        {
          "key": "wikidata_sparql",
          "title": "Wikidata SPARQL (WDQS)",
          "base": "https://query.wikidata.org/sparql?format=json&query={sparql}",
          "example": "https://query.wikidata.org/sparql?format=json&query=SELECT%20*%20WHERE%20%7B%20%3Fs%20%3Fp%20%3Fo%20%7D%20LIMIT%205",
          "cors": "yes",
          "format": "json"
        },
        {
          "key": "internet_archive_meta",
          "title": "Internet Archive Metadata",
          "base": "https://archive.org/metadata/{identifier}",
          "example": "https://archive.org/metadata/oliver2005dickens00dick",
          "cors": "often (429 may omit CORS)",
          "format": "json"
        },
        {
          "key": "openlibrary_search",
          "title": "Open Library Search",
          "base": "https://openlibrary.org/search.json?q={q}&limit={n}",
          "example": "https://openlibrary.org/search.json?q=neural%20networks&limit=10",
          "cors": "yes",
          "format": "json"
        },
        {
          "key": "gutendex_books",
          "title": "Gutendex (Project Gutenberg)",
          "base": "https://gutendex.com/books?search={q}",
          "example": "https://gutendex.com/books?search=tolstoy",
          "cors": "yes",
          "format": "json"
        },
        {
          "key": "usda_gain_reports",
          "title": "USDA FAS GAIN Reports (PDF)",
          "base": "https://apps.fas.usda.gov/newgainapi/api/Report/DownloadReportByFileName?fileName={fileName}",
          "example": "https://apps.fas.usda.gov/newgainapi/api/Report/DownloadReportByFileName?fileName=Coffee+Annual_Hanoi_Vietnam_VM2025-0018.pdf",
          "cors": "yes",
          "format": "pdf"
        }
      ],
      "asia": {
        "asean": [
          {
            "key": "od_mekong",
            "title": "Open Development Mekong (CKAN)",
            "base": "https://data.opendevelopmentmekong.net/api/3/action/{op}",
            "example": "https://data.opendevelopmentmekong.net/api/3/action/datastore_search?resource_id=<uuid>&limit=5",
            "cors": "yes",
            "format": "json"
          }
        ],
        "singapore": [
          {
            "key": "sg_data_gov",
            "title": "Singapore data.gov.sg (catalog/API)",
            "base": "https://data.gov.sg/api/3/action/{op}",
            "example": "https://data.gov.sg/api/3/action/package_search?q=traffic",
            "cors": "yes",
            "format": "json"
          }
        ],
        "malaysia": [
          {
            "key": "my_data_gov_my",
            "title": "Malaysia api.data.gov.my",
            "base": "https://api.data.gov.my/{path}",
            "example": "https://api.data.gov.my/data-catalogue",
            "cors": "yes",
            "format": "json"
          }
        ],
        "vietnam": [
          {
            "key": "hcmc_open_data",
            "title": "Ho Chi Minh City Open Data (catalog)",
            "base": "https://opendata.hochiminhcity.gov.vn/api/3/action/{op}",
            "example": "https://opendata.hochiminhcity.gov.vn/api/3/action/package_list",
            "cors": "yes",
            "format": "json"
          },
          {
            "key": "usda_gain_reports_vietnam",
            "title": "USDA GAIN Reports — Vietnam",
            "base": "https://apps.fas.usda.gov/newgainapi/api/Report/DownloadReportByFileName?fileName={fileName}",
            "example": "https://apps.fas.usda.gov/newgainapi/api/Report/DownloadReportByFileName?fileName=Exporter+Guide+Annual_Hanoi_Vietnam_VM2025-0024.pdf",
            "cors": "yes",
            "format": "pdf"
          }
        ],
        "thailand": [
          {
            "key": "usda_gain_reports_thailand",
            "title": "USDA GAIN Reports — Thailand",
            "base": "https://apps.fas.usda.gov/newgainapi/api/Report/DownloadReportByFileName?fileName={fileName}",
            "example": "https://apps.fas.usda.gov/newgainapi/api/Report/DownloadReportByFileName?fileName=Exporter+Guide+Annual_Bangkok_Thailand_TH2025-0020.pdf",
            "cors": "yes",
            "format": "pdf"
          }
        ]
      }
