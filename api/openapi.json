{
  "title": "OpenAPI",
  "version": "1.0",
  "catalog": "LLM-based headless open APIs (no-auth; direct-fetch)",
  "workflow": {
    "prefetch": {
      "default": "off",
      "override": "allowed_via_external_instruction_or_user_request"
    },
    "preflight": {
      "enabled": true,
      "mode": "on_demand",
      "trigger": [
        "manual",
        "first_call_per_endpoint"
      ],
      "probe": {
        "method": "GET",
        "max_bytes": 4096,
        "timeout_ms": 10000,
        "headers": {
          "User-Agent": "ALIAS-LLM-Fetch/1.0",
          "Accept": "application/json, text/csv, */*"
        }
      },
      "checks": {
        "cors": [
          "Access-Control-Allow-Origin",
          "Vary"
        ],
        "content_type": true,
        "rate_limit_headers": [
          "Retry-After",
          "X-RateLimit-Remaining",
          "X-RateLimit-Reset"
        ],
        "parseability": [
          "json_or_csv"
        ],
        "status_allow": [
          200,
          206
        ],
        "status_backoff": [
          429,
          503
        ]
      },
      "backoff": {
        "jitter_ms": [
          200,
          1200
        ],
        "exponential_factor": 2.0,
        "respect_retry_after": true
      }
    }
  },
  "categories": {
    "economy_finance": {
      "global": [
        {
          "key": "worldbank_indicators",
          "title": "World Bank Indicators",
          "base": "https://api.worldbank.org/v2/country/{iso3}/indicator/{indicator}",
          "example": "https://api.worldbank.org/v2/country/USA/indicator/NY.GDP.MKTP.CD?format=json&per_page=20000",
          "cors": "yes",
          "format": "json"
        },
        {
          "key": "ecb_data",
          "title": "ECB Data Portal (SDMX)",
          "base": "https://data.ecb.europa.eu/service/data/{dataset}/{key}?format=JSON",
          "example": "https://data.ecb.europa.eu/service/data/EXR/D.USD+JPY.EUR.SP00.A?format=JSON",
          "cors": "yes",
          "format": "json"
        },
        {
          "key": "eurostat_jsonstat",
          "title": "Eurostat JSON-stat",
          "base": "https://ec.europa.eu/eurostat/api/dissemination/statistics/1.0/data/{dataset}?{filters}",
          "example": "https://ec.europa.eu/eurostat/api/dissemination/statistics/1.0/data/une_rt_m?sex=T&age=Y15-74&geo=EU27_2020&unit=PC_ACT",
          "cors": "yes",
          "format": "json"
        },
        {
          "key": "oecd_sdmx",
          "title": "OECD SDMX-JSON",
          "base": "https://stats.oecd.org/sdmx-json/data/{dataset}/{dims}?contentType=json&detail=code",
          "example": "https://stats.oecd.org/sdmx-json/data/MEI_CLI/USA.CLOLITOAA.STSA.Q/all?contentType=json",
          "cors": "yes",
          "format": "json"
        },
        {
          "key": "frankfurter_fx",
          "title": "Frankfurter (ECB FX rates)",
          "base": "https://api.frankfurter.dev/{date}?from={base}&to={symbols}",
          "example": "https://api.frankfurter.dev/2025-10-01?from=EUR&to=USD,GBP",
          "cors": "yes",
          "format": "json"
        },
        {
          "key": "treasury_fiscaldata",
          "title": "U.S. Treasury Fiscal Data",
          "base": "https://api.fiscaldata.treasury.gov/services/api/fiscal_service/{path}?{filters}",
          "example": "https://api.fiscaldata.treasury.gov/services/api/fiscal_service/v2/accounting/od/treasury_rates_of_exchange?format=json",
          "cors": "yes",
          "format": "json"
        },
        {
          "key": "owid_data",
          "title": "Our World in Data (Grapher CSV)",
          "base": "https://ourworldindata.org/grapher/{path}.csv",
          "example": "https://ourworldindata.org/grapher/co2.csv",
          "cors": "yes",
          "format": "csv"
        }
      ],
      "asia": {
        "asean": [
          {
            "key": "aseanstats",
            "title": "ASEANstats Indicators",
            "base": "https://data.aseanstats.org/api/indicator/{indicator_code}",
            "example": "https://data.aseanstats.org/api/indicator/ACS_POP_TOTL",
            "cors": "yes",
            "format": "json"
          }
        ]
      }
    },

    "knowledge_content": {
