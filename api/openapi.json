    "weather_climate": {
      "global": [
        {
          "key": "open_meteo",
          "title": "Open-Meteo Forecast",
          "base": "https://api.open-meteo.com/v1/forecast?latitude={lat}&longitude={lon}&hourly=temperature_2m,relative_humidity_2m",
