          "format": "json"
        },
        {
          "key": "crossref_rest",
          "title": "Crossref REST (works)",
          "base": "https://api.crossref.org/works?query={q}&rows={n}",
          "example": "https://api.crossref.org/works?query=language%20model&rows=20",
          "cors": "yes",
          "format": "json"
        },
        {
          "key": "crossref_eventdata",
          "title": "Crossref Event Data",
          "base": "https://api.eventdata.crossref.org/v1/events?query={q}",
          "example": "https://api.eventdata.crossref.org/v1/events?query=open%20access",
          "cors": "yes",
          "format": "json"
        },
        {
          "key": "opencitations_coci",
          "title": "OpenCitations COCI",
          "base": "https://opencitations.net/api/v1/citations/{doi}",
          "example": "https://opencitations.net/api/v1/citations/10.1038%2Fnature14539",
          "cors": "yes",
          "format": "json"
        },
        {
          "key": "dblp_sparql",
          "title": "DBLP SPARQL",
          "base": "https://sparql.dblp.org/sparql?query={sparql}&format=json",
